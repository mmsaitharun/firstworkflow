<mvc:View controllerName="com.test.reviewer.reviewerpage.controller.ReviewerView" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>title}">
				<content>
					<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.core="sap.ui.core" editable="true"
						layout="ResponsiveGridLayout" id="form0">
						<sap.ui.layout.form:content>
							<sap.ui.core:Title text="Book Details" id="title0"/>
							<Label text="Title" id="label0"/>
							<Input width="100%" id="input0" value="{/product}" editable="false"/>
							<Label text="Price" id="label1"/>
							<Input width="100%" id="input2" value="{/price}" editable="false"/></sap.ui.layout.form:content>
					</sap.ui.layout.form:SimpleForm>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>